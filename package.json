{
  "type": "module",
  "name": "health-components",
  "version": "0.0.3",
  "description": "This is a react component library for health related applications.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles": "./dist/styles/components.css",
    "./styles/components.css": "./dist/styles/components.css"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:css && npx rollup -c config/rollup.config.mjs",
    "build:css": "npx @tailwindcss/cli -i ./src/styles/components.css -o ./dist/styles/components.css",
    "dev": "npm run build:css && npx rollup -c config/rollup.config.mjs -w",
    "dev:css": "npx @tailwindcss/cli -i ./src/styles/components.css -o ./dist/styles/components.css --watch",
    "clean": "npx rimraf dist",
    "test": "jest --config config/jest.config.cjs",
    "test:watch": "jest --config config/jest.config.cjs --watch",
    "storybook": "npm run build:css && storybook dev -p 6006",
    "build-storybook": "npm run build:css && storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iUtkarshtiwari/RepositoryP.git"
  },
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iUtkarshtiwari/RepositoryP/issues"
  },
  "homepage": "https://github.com/iUtkarshtiwari/RepositoryP#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.52.5",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.17"
  }
}
